<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="ExampleTheme" parent="android:Theme.Holo.Light.DarkActionBar">
        <item name="android:actionBarStyle">@style/my_actionbar_style</item>
    </style>

    <style name="my_actionbar_style" parent="@android:style/Widget.Holo.Light.ActionBar">
        <item name="android:background">#647b97</item>
        <item name="android:titleTextStyle">@style/AcBar_titleStyle</item>
    </style>

    <style name="AcBar_titleStyle">
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">#FFFFFF</item>
    </style>

    <style name="listViewMultiImageView">
        <item name="android:scaleType">centerCrop</item>
        <item name="android:layout_height">90dp</item>
        <item name="android:layout_width">90dp</item>
        <item name="android:layout_margin">2.5dp</item>
    </style>

</resources>